@model GM_Shop.Areas.Admin.Models.DanhMucSPAdmin

@{
    ViewData["Title"] = "Details";
}

<h1>Chi Tiết Danh Mục Sản Phẩm</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Mã Danh Mục Sản Phẩm
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MADMSP)
        </dd>
        <dt class = "col-sm-2">
            Tên Danh Mục Sản Phẩm
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TENDMSP)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.MADMSP" class="btn btn-warning btn-sm">Chỉnh sửa</a>
    <a asp-action="Index" class="btn btn-info btn-sm">Back to List</a>
</div>
